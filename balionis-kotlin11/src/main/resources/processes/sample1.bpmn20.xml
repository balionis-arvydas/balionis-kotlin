      <bpmndi:BPMNShape bpmnElement="end" id="BPMNShape_end">
        <omgdc:Bounds height="28.0" width="28.0" x="234.0" y="148.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="sf-01" id="BPMNEdge_sf-01">
        <omgdi:waypoint x="41.0" y="162.0"/>
